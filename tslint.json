{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "no-trailing-whitespace": false,
        "comment-format": false,
        "no-console": false,
        "one-line": false,
        "no-consecutive-blank-lines": false,
        "curly": false,
        "quotemark" : [
            true,
            "single"
        ],
        "ordered-imports": [true, {
            "import-sources-order": "any"
        }]
    },
    "rulesDirectory": []
}
